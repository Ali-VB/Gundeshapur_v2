# Test Environment Variables for Playwright Testing
# These are used specifically for authentication flow testing

# Firebase Test Configuration (using test project)
VITE_FIREBASE_API_KEY=test_firebase_api_key
VITE_FIREBASE_AUTH_DOMAIN=test-auth-domain.firebaseapp.com
VITE_FIREBASE_PROJECT_ID=test-project-id
VITE_FIREBASE_STORAGE_BUCKET=test-storage-bucket.appspot.com
VITE_FIREBASE_MESSAGING_SENDER_ID=123456789
VITE_FIREBASE_APP_ID=1:123456789:web:abcdef123456
VITE_FIREBASE_MEASUREMENT_ID=G-TEST123456

# Google API Test Configuration
VITE_GOOGLE_API_KEY=test_google_api_key
VITE_GOOGLE_CLIENT_ID=test-client-id.apps.googleusercontent.com

# Gemini API Configuration (not needed for auth tests)
VITE_GEMINI_API_KEY=test_gemini_api_key

# Test-specific variables
NODE_ENV=test
PLAYWRIGHT_TEST=true
